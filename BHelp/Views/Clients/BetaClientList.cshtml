@model IEnumerable<BHelp.Models.Client>
@{ Layout = null; }
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>BetaClientList</title>
</head>
<body>
<div>
    <table>

        @{
            var i = 0;
        }
        @foreach (var client in Model)
        {
            i = i + 1;
            <tr>
                <td>@i</td>
                <td>@Html.DisplayFor(modelItem => client.Id)</td>
                <td>@Html.DisplayFor(modelItem => client.LastName)</td>
                <td>@Html.DisplayFor(modelItem => client.FirstName)</td>
                <td>@Html.DisplayFor(modelItem => client.StreetNumber)</td>
                <td>@Html.DisplayFor(modelItem => client.StreetName)</td>
                <td>@Html.DisplayFor(modelItem => client.Zip)</td>
            </tr>
        }
    </table>
</div>
<br />
<div>
    @Html.ActionLink("Return to Dashboard", "Index", "Home", new { }, null)
</div>
</body>
</html>
