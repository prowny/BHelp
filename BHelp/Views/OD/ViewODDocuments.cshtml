@model BHelp.ViewModels.DocumentsViewModel
@{ Layout = null; }
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ViewDocuments</title>
</head>
<body>
<div>
    <b>OD Documents</b>
</div>
<br/>
<div>
    @for (int i = 0; i < Model.DocNamesUpperBound; i++)
    {
        <div>
            @Html.Label(Model.DocNames[i, 0])
            <a href=@Model.DocNames[i, 1] target="_blank">
                <img src="/Images/PDF10.png" style="border: none" alt=""></a><br/>
        </div>
    }
</div>
<br/>
<div>
    @Html.ActionLink("Return to Dashboard", "ReturnToDashboard", new { })
</div>
</body>
</html>
